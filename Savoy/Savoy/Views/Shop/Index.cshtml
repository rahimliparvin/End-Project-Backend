    <section id="categories-search-filter">
        <div class="container-fluid my-3">
            <div class="row">
                <div class="col-lg-9">
                    <div class="categoriesname">
                        <ul>
                            <li data-id="All" class="active">All<span>/</span></li>
                          
                            @foreach (var category in Model.Categories)
                            {
                                  <li data-id="Bags&Backpacks">@category.Name<span>/</span></li>
                            }
                            
                        </ul>
                    </div>
                </div>

                <div class="col-lg-3">
                    <div class="filtersort">
                        <ul>
                            <li>Filter<span>/</span></li>
                            <li>Search <i class="fa-solid fa-magnifying-glass"></i></li>
                        </ul>
                    </div>

                </div>
            </div>

            <div class="col-lg-12">
                <div class="searcharea d-none">
                    <input placeholder="Search products" type="text"><i class="fa-solid fa-xmark"></i>
                </div>

                <div class="filter d-none">
                    <div class="col-lg-3">
                        <h5>Sort By</h5>
                        <ul>
                            <li>Default</li>
                            <li>Popularity</li>
                            <li>Average rating</li>
                            <li>Newness</li>
                            <li>Price: Low to High</li>
                            <li>Price: High to Low</li>
                        </ul>
                    </div>
                    <div class="col-lg-3">
                        <h5>Price</h5>
                        <ul>
                            <li>All</li>
                            <li>$0 - $50</li>
                            <li>$50 - $100</li>
                            <li>$100 - $150</li>
                            <li>$150 - $200</li>
                            <li>$200+</li>
                        </ul>
                    </div>
                    <div class="col-lg-3">
                        <h5>Color</h5>
                        <ul class="color">
                          
                            @foreach (var color in Model.Colors)
                            {
                                  <li><i class="fa-solid fa-circle"></i><span>@color.Name</span></li>
                            }
                            
                        </ul>
                    </div>
                    <div class="col-lg-3">
                        <div class="tags">
                            <h5>Tags</h5>
                           
